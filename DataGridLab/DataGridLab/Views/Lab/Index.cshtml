@{
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>
    <title>Lab</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/jquery", "~/bundles/jqueryui", "~/bundles/knockout")
</head>
<body>
    <div>
        <h3>Normal Data Bind</h3>
        <table id="normal">
            <thead>
                <tr>
                    <th>ID</th>
                    <th>Company</th>
                    <th>Name</th>
                </tr>
            </thead>
            <tbody data-bind="foreach: data">
                <tr>
                    <td data-bind="text: ID"></td>
                    <td data-bind="text: Company"></td>
                    <td data-bind="text: Name"></td>
                </tr>
            </tbody>
        </table>
    </div>

    <div>
        <h3>Data Binding with Template</h3>
        <table data-bind="table: data"></table>
    </div>

    @Scripts.Render("~/bundles/lab")
</body>
</html>
